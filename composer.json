{
    "name": "apdevs/bbmcore",
    "description": "Nucleo para front con Backbone-Marionette para Laravel",
    "license": "proprietary",
    "authors": [
        {
            "name": "José Manuel Ruiz Pérez",
            "email": "jmanuel@apdevs.com"
        }
    ],
    "homepage": "https://gitlab.com/apdevs/bbmcore",
    "keywords": ["APDevs", "Laravel", "Backbone", "Marionette", "Core"],
    "require": {
        "illuminate/support": ">=5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "APDevs\\BackboneMarionetteCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "APDevs\\BackboneMarionetteCore\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "APDevs\\BackboneMarionetteCore\\BackboneMarionetteCoreServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
